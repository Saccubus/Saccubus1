<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<!--
	アスペクト比共通設定　さきゅばす 1.39以降
	mp4 H264 640x384 q23, 44100Hz 128kbps AAC
-->
<properties>
	<entry key="EXT">mp4</entry>
	<entry key="MAIN"></entry>
	<entry key="IN"></entry>
	<entry key="OUT">-threads 0 -s 640x384 -acodec libvo_aacenc -ab 128k -ar 44100 -ac 2 -vcodec libx264 -crf 23 -b 1400k -bt 2000k -maxrate 2000k -bufsize 2000k -async 1 -coder 1 -sws_flags lanczos -flags +loop -cmp +chroma -partitions +parti4x4+partp8x8+partb8x8 -me_method umh -subq 8 -me_range 16 -g 250 -keyint_min 25 -sc_threshold 40 -i_qfactor 0.71 -b_strategy 2 -qcomp 0.6 -qmin 10 -qmax 51 -qdiff 4 -bf 3 -refs 3 -directpred 3 -trellis 1 -flags2 +wpred+mixed_refs+dct8x8+fastpskip -samx</entry>
</properties>
