<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<!-- jpg連続出力オプション(N55686用) -->
<properties>
<entry key="EXT">%d.jpg</entry>
<entry key="MAIN"></entry>
<entry key="IN"></entry>
<!-- -r にはjpg出力レート(/sec)を指定する -->
<entry key="OUT">-r 1 -s 640x360 -an -flags +bitexact -f image2 -vfilters pad=672:384:16:12</entry>
</properties>
