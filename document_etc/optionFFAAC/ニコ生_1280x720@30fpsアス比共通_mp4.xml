<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<!--
	アスペクト比共通設定　さきゅばす 1.60以降 ffmpeg 3.0以降
	mp4 H264 1280x720 30fps 44100Hz 128kbps AAC
-->
<properties>
	<entry key="EXT">mp4</entry>
	<entry key="MAIN"></entry>
	<entry key="IN">-analyzeduration 10M</entry>
	<entry key="OUT">-threads 0 -s 1280x720 -acodec aac -ab 128k -ar 44100 -ac 2 -vcodec libx264 -crf 18 -async 1 -aspect 16:9 -samx -pix_fmt yuv420p -vf fps=30</entry>
</properties>
